{
  "name": "@silvenga/blog",
  "private": true,
  "license": "MIT",
  "repository": "git@github.com:Silvenga/silvenga.com",
  "author": "Mark Lopez <m@silvenga.com>",
  "packageManager": "yarn@4.2.2",
  "type": "module",
  "browserslist": "> 1%, not dead, Firefox ESR",
  "importSort": {
    ".ts,.tsx": {
      "style": "import-sort-style-module-compact",
      "options": {}
    }
  },
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "parcelIgnore": [
    "http://",
    "https://",
    "ftp://",
    "/atom-posts.xml"
  ],
  "alias": {
    "./img/*": "./.cache/eleventy/img/$1"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "staticFiles": [
    {
      "staticPath": ".cache/eleventy/robots.txt",
      "staticOutPath": ""
    },
    {
      "staticPath": ".cache/eleventy/.nojekyll",
      "staticOutPath": ""
    },
    {
      "staticPath": ".cache/eleventy/atom-posts.xml",
      "staticOutPath": ""
    }
  ],
  "scripts": {
    "tailwindcss": "tailwindcss",
    "nodemon": "nodemon",
    "eleventy": "eleventy",
    "eleventy:watch": "nodemon --watch './src/**/*' --watch eleventy.config.ts --ext '*' --delay 0  --exec yarn eleventy:build",
    "eleventy:build": "tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts",
    "parcel": "parcel",
    "parcel:start": "parcel --port 9999 --config .parcelrc --cache-dir .cache/parcel '.cache/eleventy/**/*.html' '.cache/eleventy/*.xml'",
    "parcel:build": "parcel build --config .prod.parcelrc --cache-dir .cache/parcel '.cache/eleventy/**/*.html' '.cache/eleventy/*.xml'",
    "lint": "eslint --max-warnings=0 src",
    "start": "concurrently \"yarn:eleventy:watch\" \"yarn:parcel:start\" --restart-tries -1",
    "clean": "rimraf ./dist && rimraf ./.cache",
    "build": "yarn clean && yarn eleventy:build && yarn parcel:build"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0-alpha.11",
    "@11ty/eleventy-img": "^5.0.0-beta.4",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@parcel/compressor-brotli": "2.11.0",
    "@parcel/optimizer-data-url": "2.11.0",
    "@parcel/packager-raw-url": "2.11.0",
    "@parcel/packager-xml": "2.11.0",
    "@parcel/reporter-bundle-analyzer": "2.11.0",
    "@parcel/transformer-inline-string": "2.11.0",
    "@parcel/transformer-sass": "2.11.0",
    "@parcel/transformer-webmanifest": "2.11.0",
    "@parcel/transformer-xml": "2.11.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/11ty__eleventy-img": "^4",
    "@types/luxon": "^3",
    "@types/markdown-it": "^14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "eleventy-plugin-time-to-read": "^1.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "luxon": "^3.4.4",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.0.1",
    "nodemon": "^3.1.3",
    "parcel": "2.11.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "parcel-resolver-ignore": "^2.2.0",
    "postcss": "^8.4.38",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.15.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.0.18"
  }
}
